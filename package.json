{
  "name": "snitch-node-client",
  "version": "0.0.0",
  "description": "Streamdal's Snitch Node Client",
  "keywords": [
    "typescript",
    "node",
    "data quality"
  ],
  "type": "module",
  "scripts": {
    "build": "npm run clean && npm run type:dts && npm run build:main",
    "build:main": "swc ./src -d ./build",
    "clean": "rimraf build coverage nyc_output",
    "cz": "cz",
    "lint": "eslint src --ext .ts --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "setup": "tsx ./scripts/setup.ts",
    "spell:check": "cspell \"{README.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
    "start": "tsc-watch --onSuccess \"node --experimental-wasi-unstable-preview1 ./build/index.js\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "type:dts": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "type:check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streamdal/node-dataqual.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streamdal/node-dataqual/issues"
  },
  "homepage": "https://github.com/streamdal/node-dataqual#readme",
  "dependencies": {
    "@grpc/grpc-js": "^1.8.17",
    "@grpc/proto-loader": "^0.7.7",
    "@protobuf-ts/grpc-transport": "^2.9.0",
    "@streamdal/snitch-protos": "^0.0.55",
    "@types/rwlock": "^5.0.3",
    "dotenv": "^16.3.1",
    "rwlock": "^5.0.0"
  },
  "devDependencies": {
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/github": "^9.0.3",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.3",
    "@types/node": "^18.16.3",
    "@types/prompts": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "c8": "^8.0.0",
    "cspell": "^6.31.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "prompts": "^2.4.2",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.5",
    "slugify": "^1.6.6",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  }
}
